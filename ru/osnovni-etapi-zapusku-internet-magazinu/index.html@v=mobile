        <form method="get" data-action="/ru/katalog/search/">
            <button type="submit" class="search__button j-search-button">
                <svg class="icon icon--search"><use xlink:href="#icon-search"></use></svg>
            </button>
            <input class="search__input j-search-input" id="search_uida7cf4a8c13d36b5902e8487f2cdd0ef4" type="text" name="q" placeholder="поиск товаров" autocomplete="off" value="">
            <button class="search__close j-search-close" type="reset">
                <svg class="icon icon--cross"><use xlink:href="#icon-cross"></use></svg>
            </button>
