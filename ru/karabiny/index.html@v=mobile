            <input class="search__input j-search-input" id="search_uidca74f6a8c82a14f68c6b59d29877ce0a" type="text" name="q" placeholder="поиск товаров" autocomplete="off" value="">
            <button class="search__close j-search-close" type="reset">
                <svg class="icon icon--cross"><use xlink:href="#icon-cross"></use></svg>
            </button>
