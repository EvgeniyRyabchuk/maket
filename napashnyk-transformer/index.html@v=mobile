            <input class="search__input j-search-input" id="search_uidd6629649469523a27dffc498d6bc8793" type="text" name="q" placeholder="пошук товарів" autocomplete="off" value="">
            <button class="search__close j-search-close" type="reset">
                <svg class="icon icon--cross"><use xlink:href="#icon-cross"></use></svg>
